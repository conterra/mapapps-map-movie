{
    "dataform-version": "1.0.0",
    "type": "gridpanel",
    "showLabels": true,
    "cols": 1,
    "children": [
        {
            "type": "accordionpanel",
            "field": "selectedChild",
            "children": [
                {
                    "type": "gridpanel",
                    "showLabels": true,
                    "cols": 1,
                    "title": "1. Layer (required)",
                    "children": [{
                            "type": "textbox",
                            "title": "Titel",
                            "field": "layer1.title"
                        },
                        {
                            "type": "textbox",
                            "title": "URL",
                            "field": "layer1.url",
                            "required": true,
                            "regex": "url",
                            "size": {
                                "w": 400
                            }
                        },
                        {
                            "type": "radiobutton",
                            "title": "ArcGIS MapServer (Tiled)",
                            "field": "layer1.type",
                            "value": "AGS_TILED"
                        },
                        {
                            "type": "textbox",
                            "title": "URL des Bildes",
                            "field": "layer1.thumbnail",
                            "size": {
                                "w": 400
                            }
                        }]
                },
                {
                    "type": "gridpanel",
                    "showLabels": true,
                    "cols": 1,
                    "title": "2. Layer (required)",
                    "required": true,
                    "children": [{
                            "type": "textbox",
                            "title": "Titel",
                            "field": "layer2.title"
                        },
                        {
                            "type": "textbox",
                            "title": "URL",
                            "field": "layer2.url",
                            "required": true,
